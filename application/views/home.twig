{% extends "layout.twig" %}

{% block title %} {{parent()}} - Home{% endblock title %}

{% block container%}
<div class="span12"> 
    <div id="myCarousel" class="carousel slide">
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>
        <!-- Carousel items -->
        <div class="carousel-inner" style="height: 200px">
            <div class="active item"><img src="{{base_url()}}assets/img/slider/slider1.jpg" alt="" height="200"/></div>
            <div class="item"><img src="{{base_url()}}assets/img/slider/slider2.jpg" alt=""/></div>
            <div class="item"><img src="{{base_url()}}assets/img/slider/slider3.jpg" alt=""/></div>
            <div class="item"><img src="{{base_url()}}assets/img/slider/slider4.jpg" alt=""/></div>
            <div class="item"><img src="{{base_url()}}assets/img/slider/slider5.jpg" alt=""/></div>
            <div class="item"><img src="{{base_url()}}assets/img/slider/slider6.jpg" alt=""/></div>
            <div class="item"><img src="{{base_url()}}assets/img/slider/slider7.jpg" alt=""/></div>
        </div>
        <!-- Carousel nav -->
        <a class="carousel-control left" href="#myCarousel" data-slide="prev">&lsaquo;</a>
        <a class="carousel-control right" href="#myCarousel" data-slide="next">&rsaquo;</a>
    </div>
    </div>
{% endblock container%}